	<comment>English (US) Localization File, &lt; is the less than or left arrow sign</comment>
	<entry key="command.motd.syntax.console">/motd [new motd]</entry>
	<entry key="command.motd.syntax.player">/motd [new motd]</entry>
	<entry key="command.motd.info.console">Get/set the message of the day</entry>
	<entry key="command.motd.info.player">Get/set the message of the day</entry>

	<entry key="command.rules.syntax.console">/rules [&lt;number> &lt;new rule>|remove]</entry>
	<entry key="command.rules.syntax.player">/rules [&lt;number> &lt;new rule>|remove]</entry>
	<entry key="command.rules.info.console">Get/set the rules of the server</entry>
	<entry key="command.rules.info.player">Get/set the rules of the server</entry>

	<entry key="command.butcher.syntax.player">/butcher [radius] [&lt;x> &lt;y> &lt;z>]</entry>
	<entry key="command.butcher.info.player">Kill enemies around you/the specified point within the radius</entry>

	<entry key="command.remove.syntax.player">/remove [radius] [&lt;x> &lt;y> &lt;z>]</entry>
	<entry key="command.remove.info.player">Remove all item entities around you/the specifies point within the radius</entry>

	<entry key="command.kill.syntax.console">/kill &lt;player></entry>
	<entry key="command.kill.syntax.player">/kill [player]</entry>
	<entry key="command.kill.info.console">Kill the specified player</entry>
	<entry key="command.kill.info.player">Kill yourself or the specified player</entry>

	<entry key="command.smite.syntax.console">/smite &lt;player></entry>
	<entry key="command.smite.syntax.player">/smite [me|&lt;player>]</entry>
	<entry key="command.smite.info.console">Strike the specified player with lightning</entry>
	<entry key="command.smite.info.player">Strike where you are looking, yourself, or another player with lightning</entry>

	<entry key="command.home.syntax.player">/home [here|&lt;x> &lt;y> &lt;z>]</entry>
	<entry key="command.home.info.player">Teleport you to/sets your home</entry>

	<entry key="command.back.syntax.player">/home</entry>
	<entry key="command.back.info.player">Teleport you your last death point</entry>

	<entry key="command.restart.syntax.console">/restart</entry>
	<entry key="command.restart.info.console">Restart the server</entry>

	<entry key="command.serverdo.syntax.player">/serverdo &lt;command> [arg1]...</entry>
	<entry key="command.serverdo.info.player">Run a console command as a player</entry>

